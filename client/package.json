{
  "name": "client",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && ng serve",
    "watch": "ng serve --watch",
    "debug": "ng serve -o",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "build-full-prod": "npm run commit-prep && ng build --prod",
    "test": "ng test --browsers=ChromeHeadless --reporters=spec",
    "lint": "./node_modules/.bin/eslint \"src/**/*.ts\" --ignore-path \".eslintignore\"",
    "lint-fix": "./node_modules/.bin/eslint \"src/**/*.ts\" --ignore-path \".eslintignore\" --fix",
    "lint-errors": "./node_modules/.bin/eslint \"src/**/*.ts\" --quiet",
    "e2e": "ng e2e",
    "commit-prep": "npm run test && npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.8",
    "@angular/common": "~9.0.0-rc.8",
    "@angular/compiler": "~9.0.0-rc.8",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.0.0-rc.8",
    "@angular/platform-browser": "~9.0.0-rc.8",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.8",
    "@angular/router": "~9.0.0-rc.8",
    "@typescript-eslint/eslint-plugin": "~2.15.0",
    "@typescript-eslint/parser": "~2.15.0",
    "eslint": "~6.8.0",
    "lodash": "~4.17.15",
    "rxjs": "~6.5.3",
    "tslib": "~1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.7",
    "@angular/cli": "~9.0.0-rc.8",
    "@angular/compiler-cli": "~9.0.0-rc.8",
    "@angular/language-service": "~9.0.0-rc.8",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "~4.14.149",
    "@types/node": "~13.1.6",
    "@typescript-eslint/eslint-plugin": "~2.15.0",
    "@typescript-eslint/parser": "~2.15.0",
    "codelyzer": "~5.1.2",
    "eslint": "~6.8.0",
    "eslint-plugin-import": "~2.19.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.1",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.2",
    "karma-spec-reporter": "~0.0.32",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.6.4",
    "typescript-tslint-plugin": "~0.5.5"
  }
}
