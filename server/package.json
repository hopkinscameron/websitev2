{
  "name": "server",
  "version": "2.0.0",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/index.js",
    "build-and-start": "npm run build && npm run serve",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "serve-debug": "nodemon --inspect dist/index.js",
    "test": "ng test --browsers=ChromeHeadless --reporters=spec",
    "lint": "./node_modules/.bin/eslint \"**/*.ts\" --ignore-path \".eslintignore\"",
    "lint-fix": "./node_modules/.bin/eslint \"**/*.ts\" --ignore-path \".eslintignore\" --fix",
    "lint-errors": "./node_modules/.bin/eslint \"**/*.ts\" --quiet",
    "e2e": "ng e2e",
    "commit-prep": "ng test --browsers=ChromeHeadless --reporters=spec && npm run lint"
  },
  "private": true,
  "dependencies": {
    "body-parser": "~1.19.0",
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "~2.8.5",
    "dotenv": "~8.2.0",
    "eslint": "~6.8.0",
    "events": "~3.1.0",
    "express": "~4.17.1",
    "express-async-handler": "~1.1.4",
    "express-validation": "~1.0.3",
    "formidable": "~1.2.1",
    "glob": "^7.1.6",
    "helmet": "~3.21.2",
    "http-errors": "~1.7.3",
    "lodash": "^4.17.15",
    "method-override": "~3.0.0",
    "morgan": "~1.9.1",
    "nodemon": "~2.0.2",
    "path": "~0.12.7",
    "rxjs": "~6.5.4",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "~4.1.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.1.6",
    "@typescript-eslint/eslint-plugin": "~2.15.0",
    "@typescript-eslint/parser": "~2.15.0",
    "codelyzer": "~5.2.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "ts-node": "~8.6.0",
    "tslint": "~5.20.1",
    "typescript": "~3.7.4",
    "typescript-tslint-plugin": "~0.5.5"
  }
}
